---
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";
import Hero from "../components/Hero.section.astro";
import HomeBlogs from "../components/HomeBlogs.astro";
import WeAre from "../components/WeAre.astro";
import AboutEOR from "../components/AboutEOR.astro";
import Partners from "../components/Partners.astro";
import ClientTestimonial from "../components/ClientTestimonial.astro";
import Services from "../components/Services.astro";
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body class="overflow-y-scroll no-scrollbar">
    <Navbar></Navbar>
    <Hero></Hero>
    <WeAre></WeAre>
    <Services></Services>
    <AboutEOR></AboutEOR>
    <HomeBlogs></HomeBlogs>
    <Partners></Partners>
    <ClientTestimonial></ClientTestimonial>
    <section class="bg-white dark:bg-gray-900">
        <div class="py-8 px-4 mx-auto max-w-screen-xl sm:py-16 lg:px-6">
            <div class="mx-auto max-w-screen-sm text-center">
                <h2 class="mb-4 text-4xl tracking-tight font-extrabold leading-tight text-gray-900 dark:text-white">Start your free trial today</h2>
                <p class="mb-6 font-light text-gray-500 dark:text-gray-400 md:text-lg">Try Flowbite Platform for 30 days. No credit card required.</p>
                <a href="#" class="text-gray-500 bg-primary-700 hover:bg-primary-800 focus:ring-4 focus:ring-primary-300 font-medium rounded-lg text-sm px-5 py-2.5 mr-2 mb-2 dark:bg-primary-600 dark:hover:bg-primary-700 focus:outline-none dark:focus:ring-primary-800">Free trial for 30 days</a>
            </div>
        </div>
    </section>
    <Footer></Footer>
    <!-- TW Elements is free under AGPL, with commercial license required for specific uses. See more details: https://tw-elements.com/license/ and contact us for queries at tailwind@mdbootstrap.com --> 

</body>
</html>
<style>
	.padding{
		height: 800px;
		background-color: rgb(81, 81, 81);
	}
</style>
